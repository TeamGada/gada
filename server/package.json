    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "fs-extra": "^10.1.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.1.5",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
